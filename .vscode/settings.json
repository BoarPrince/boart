{
  "[*.cpt]": {
    "editor.defaultFormatter": "darkriszty.markdown-table-prettify"
  },
  "[*.spec]": {
    "editor.defaultFormatter": "darkriszty.markdown-table-prettify"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[*.html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "html.suggest.html5": true,
  "emmet.includeLanguages": {
    "njk": "html5",
    "nunjucks": "html5"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "darkriszty.markdown-table-prettify",
    "editor.unicodeHighlight.includeComments": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "custom-language-properties": {
    "Markdown.comments.lineComment": "comment *",
    "markdown.comments.lineComment": "comment *",
    "gauge.comments.lineComment": "comment *"
  },
  "coverage-gutters.showGutterCoverage": true,
  "coverage-gutters.showLineCoverage": true,
  "coverage-gutters.showRulerCoverage": false,
  "editor.formatOnSave": true,
  "files.associations": {
    "*.cpt": "gauge",
    "*.json": "jsonc",
    "*.spec": "gauge",
    "*.html": "html"
  },
  "files.autoSave": "off",
  "jest.autoRun": {
    "watch": false
  },
  "jest.debugMode": false,
  // "jest.jestCommandLine": "./node_modules/.bin/jest --coverage --runInBand",
  "jest.jestCommandLine": "./node_modules/.bin/jest --runInBand",
  "jest.showCoverageOnLoad": true,
  "jest.testExplorer": {
    "enabled": true
  },
  "jestTestExplorer.debugOutput": "internalConsole",
  "jestTestExplorer.flattenExplorer": false,
  "jestTestExplorer.hideEmptyProjects": false,
  "jestTestExplorer.logpanel": true,
  "jestTestExplorer.pathToJest": "./node_modules/.bin/jest --coverage --runInBand",
  "search.exclude": {
    "**/coverage": true,
    "**/dist": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/gauge.log": true,
    "**/gauge/node_modules": true,
    "**/reports": true,
    "**/tests/util/*.js": true
  },
  "terminal.integrated.cursorWidth": 20,
  "testExplorer.addToEditorContextMenu": true,
  "testExplorer.useNativeTesting": true,
  "typescript.tsdk": "./node_modules/typescript/lib",
  "window.title": "B O A R - T",
  "sonarlint.connectedMode.project": {
    "connectionId": "",
    "projectKey": ""
  },
  "files.autoSaveDelay": 5000000000,
  "commentTranslate.hover.enabled": false,
}