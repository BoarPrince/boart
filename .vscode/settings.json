{
  "[.cpt]": {
    "editor.defaultFormatter": "darkriszty.markdown-table-prettify"
  },
  "[*.spec]": {
    "editor.defaultFormatter": "darkriszty.markdown-table-prettify"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "darkriszty.markdown-table-prettify"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "coverage-gutters.showGutterCoverage": true,
  "coverage-gutters.showLineCoverage": true,
  "coverage-gutters.showRulerCoverage": false,
  "editor.formatOnSave": true,
  "eslint.format.enable": true,
  "eslint.useESLintClass": true,
  "files.associations": {
    "*.cpt": "gauge",
    "*.json": "jsonc",
    "*.spec": "gauge"
  },
  "files.autoSave": "off",
  "jest.autoRun": {
    "watch": false
  },
  "jest.debugMode": false,
  "jest.jestCommandLine": "./node_modules/.bin/jest --coverage --runInBand",
  "jest.showCoverageOnLoad": true,
  "jest.testExplorer": {
    "enabled": true
  },
  "jestTestExplorer.debugOutput": "internalConsole",
  "jestTestExplorer.flattenExplorer": false,
  "jestTestExplorer.hideEmptyProjects": false,
  "jestTestExplorer.logpanel": true,
  "jestTestExplorer.pathToJest": "./node_modules/.bin/jest --coverage --runInBand",
  "search.exclude": {
    "**/coverage": true,
    "**/dist": true,
    "**/gauge": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/reports": true,
    "**/tests/util/*.js": true
  },
  "terminal.integrated.cursorWidth": 20,
  "testExplorer.addToEditorContextMenu": true,
  "testExplorer.useNativeTesting": false,
  "window.title": "B O A R - T",
  "karmaTestExplorer.nonHeadlessModeEnabled": true
}
