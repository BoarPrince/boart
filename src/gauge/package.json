{
  "name": "boart-gauge-integration",
  "description": "Starter template for writing JavaScript tests for Gauge",
  "scripts": {
    "test": "gauge run --tags=\"md-3.1\"",
    "test:": "gauge run",
    "*test:doc": "export runtime_environent=local && gauge run --tags=\"#doc#\"",
    "test:doc": "export runtime_environent=local && gauge run",
    "*test:local": "export runtime_environent=local && gauge run --tags=\"(!env:development & !env:staging | env:local) & onboarding & manual\"",
    "test:local": "export runtime_environent=local && gauge run --tags=\"(!env:development & !env:staging | env:local) & md-5.16\"",
    "_test:dev": "export runtime_environent=development && gauge run --tags=\"onboarding\"",
    "__test:dev": "export runtime_environent=development && gauge run --tags=\"masterdata\"",
    "*test:stage": "export runtime_environent=staging && gauge run --tags=\"md-5.14\"",
    "test:dev": "export runtime_environent=development && gauge run --tags=\"md-5.16\"",
    "__test:stage": "export runtime_environent=staging && gauge run --tags=\"masterdata\"",
    "test:stage:sync": "export runtime_environent=staging && gauge run --tags=\"masterdata-sync\"",
    "test:stage:onboarding": "export runtime_environent=staging && gauge run --tags=\"onboarding\"",
    "test:stage": "export runtime_environent=staging && gauge run --tags=\"masterdata\"",
    "test:stage:register": "export runtime_environent=staging && gauge run --tags=\"ob-10.test\"",
    "test:stage:manual": "export runtime_environent=staging && gauge run --tags=\"manual\"",
    "test:stage:add-user": "export runtime_environent=staging && gauge run --tags=\"md-5.2\"",
    "update": "rm -rf node_modules/@boart && npm update",
    "update:and:test": "npm run update && npm run test"
  },
  "dependencies": {
    "@boart/basic": "*",
    "@boart/core": "*",
    "@boart/core-impl": "*",
    "@boart/protocol": "*",
    "@boart/execution": "*",
    "taiko": "latest"
  }
}
